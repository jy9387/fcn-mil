net: "examples/mil/train_val.prototxt"
# lr for fine-tuning should be lower than when starting from scratch
#debug_info: true
base_lr: 1e-8
lr_policy: "step"
gamma: 0.1
iter_size: 5
# stepsize should also be lower, as we're closer to being done
stepsize: 5000
display: 2
max_iter: 15000
momentum: 0.9
weight_decay: 0.0002
snapshot: 1000
snapshot_prefix: "examples/mil/net"
solver_mode: GPU
#solver_mode: CPU